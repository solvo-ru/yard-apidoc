{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Профиль пользователя",
  "type": "object",
  "required": [
    "id","username"
  ],
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "username": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "familyName": {
      "type": "string"
    }
  }
}
